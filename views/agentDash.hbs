<div class="card">
    <div class="card-header">
        <h5 class="card-title">Send / Receive Cycles</h5>
    </div>

<!--    <div class="card-body">-->
<!--        Modules-->
<!--    </div>-->

    <table class="table table-bordered table-hover datatable-highlight">
        <thead>
        <tr>
            <th>TS</th>
            <th>Time</th>
            <th>Event</th>
            <th>Message</th>
            <th>Sender</th>
            <th>Award</th>
            <th>Valid receives</th>
        </tr>
        </thead>
        <tbody>


        {{#each cycles}}
            <tr>
                <td>{{ts}}</td>
                <td>{{fromNow}}</td>
                <td>{{event}}</td>
                <td>{{mic}}</td>
                <td>{{gatewayId}}</td>
                <td>{{award}}</td>
                <td>{{jsonValRec}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>

